{
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": [
      "esnext"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noUncheckedIndexedAccess": true,
    "outDir": "dist",
    "target": "esnext",
    "useUnknownInCatchVariables": true
  },
  "include": [
    "./src/*"
  ]
}